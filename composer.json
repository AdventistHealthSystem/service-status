{
    "name": "corycollier/server-status",
    "description": "Project to serve the status of services on a server",
    "type": "library",
    "license": "MIT",
    "version" : "2.2.0",
    "authors": [
        {
            "name": "Cory Collier",
            "email": "corycollier@corycollier.com",
            "role" : "Lead Developer"
        }
    ],
    "prefer-stable" : true,
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^6.2"
    },
    "autoload": {
       "psr-4": {
           "ServerStatus\\": "src/",
           "ServerStatus\\Tests\\" : "tests/"
       }
   },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "phploc/phploc": "^3.0",
        "squizlabs/php_codesniffer": "^2.6",
        "theseer/phpdox": "0.8.1.1",
        "sebastian/phpcpd": "^2.0",
        "pdepend/pdepend": "^2.2",
        "phpmd/phpmd": "^2.4",
        "satooshi/php-coveralls": "^1.0"
    }
}
